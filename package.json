{
  "name": "faker",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build:reflection": "NEXUS_SHOULD_EXIT_AFTER_REFLECTION=true ts-node ./src/api/schema",
    "dev": "env-cmd -f ./config/dev.env ts-node-dev --transpile-only ./src/api/app",
    "dev:migrate": "env-cmd -f ./config/dev.env prisma  db push  ",
    "dev:typecheck": "tsc --watch --noEmit",
    "format": "npx prettier --write './**/*.{ts,md}'",
    "start": "  node build/api/app",
    "data:seed": "env-cmd -f ./config/dev.env ts-node src/seed/index.ts",
    "data:mainSeed": " ts-node src/seed/mainSeed.ts",
    "server": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "playground": "graphql playground",
    "prisma": "prisma",
    "graphql": "graphql",
    "build": "rm -rf build &&  yarn -s generate && tsc",
    "style": "prettier --write 'src/**/*'",
    "generate": "yarn -s generate:prisma && yarn -s generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "NODE_ENV=development ts-node --transpile-only ./src/api/schema"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@prisma/client": "3.5.0",
    "apollo-server-express": "^3.5.0",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cron-job-manager": "^2.2.1",
    "date-fns": "^2.23.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "graphql": "^15.5.2",
    "graphql-middleware": "^6.1.12",
    "graphql-scalars": "^1.13.6",
    "graphql-shield": "^7.5.0",
    "graphql-subscriptions": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "mailgun.js": "^4.1.2",
    "make-promises-safe": "^5.1.0",
    "moment": "^2.29.1",
    "nexus": "^1.1.0",
    "nexus-plugin-prisma": "^0.35.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.2",
    "subscriptions-transport-ws": "^0.11.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "16.11.26",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "env-cmd": "^10.1.0",
    "faker": "^5.5.3",
    "graphql-request": "^3.6.1",
    "prettier": "2.4.1",
    "prisma": "^3.5.0",
    "ts-node": "10.7.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "4.6.2"
  }
}
